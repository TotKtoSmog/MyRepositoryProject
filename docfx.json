{
  "metadata": [
    {
      "src": [
        {
          "files": [ "MyRepositoryProject/MyRepositoryProject.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "statictoc"
    ],
    "globalMetadata": {
      "_appName": "MyRepositoryProject",
      "_appTitle": "MyRepositoryProject",
      "_enableSearch": false,
      "pdf": false
    }
  }
}
